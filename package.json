{
  "name": "radford",
  "version": "0.0.6",
  "description": "A service container and dependency manager for your application",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha --require babel-core/register",
    "prepublish": "babel src --out-dir dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shawndellysse/radford.git"
  },
  "keywords": [
    "service",
    "dependency-injection",
    "container"
  ],
  "author": "Shawn Dellysse",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shawndellysse/radford/issues"
  },
  "homepage": "https://github.com/shawndellysse/radford#readme",
  "devDependencies": {
    "babel-cli": "6.6.5",
    "babel-core": "6.7.4",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-preset-es2015": "6.6.0",
    "mocha": "2.4.5"
  },
  "dependencies": {
    "clone": "1.0.2"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "babel-plugin-add-module-exports"
    ]
  }
